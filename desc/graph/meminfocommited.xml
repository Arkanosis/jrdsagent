<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
	<name>MeminfoRAMCommitted</name>
	<graphName>meminforamcommited</graphName>
	<graphTitle>Meminfo commited on ${host}</graphTitle>
	<unit>
		<binary />
	</unit>
	<verticalLabel>Bytes</verticalLabel>
	<!-- Swap usage -->
	<add>
		<name>SwapTotal</name>
	</add>
	<add>
		<name>SwapFree</name>
	</add>
	<add>
		<rpn>SwapTotal, SwapCached, -, SwapFree, -</rpn>
		<graphType>area</graphType>
		<color>red</color>
		<legend>Swap used</legend>
	</add>
	<add>
		<name>SwapCached</name>
		<graphType>stack</graphType>
		<color>blue</color>
		<legend>Memory in both RAM and swap</legend>
	</add>

	<add>
		<name>Slab</name>
	</add>
	<add>
		<name>MemTotal</name>
	</add>
	<add>
		<name>Buffers</name>
	</add>
	<add>
		<name>Cached</name>
	</add>
	<add>
		<name>MemFree</name>
	</add>
	<add>
		<legend>Other memory</legend>
	</add>
	<add>
		<rpn>MemTotal, Buffers, -, Cached, -, MemFree, -, Slab, -, SwapCached, -</rpn>
		<graphType>stack</graphType>
		<color>green</color>
		<legend>Used memory not in swap</legend>
	</add>

	<add>
		<dsName>Committed_AS</dsName>
		<graphType>line</graphType>
		<color>black</color>
		<legend>Commited memory</legend>
	</add>

	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>RAM Memory usage</pathstring>
		<pathstring>Committed Memory informations</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>Committed Memory informations</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
