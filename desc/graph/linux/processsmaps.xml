<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
    <name>ProcessSmaps</name>
    <graphName>prsmaps-${index}</graphName>
    <graphTitle>Exact memory usage for processes ${index} on host ${host}</graphTitle>
    <unit>
        <binary />
    </unit>
    <verticalLabel>Bytes</verticalLabel>
    <add>
        <name>mf:Anonymous</name>
    </add>
    <add>
        <name>anon:Anonymous</name>
    </add>
    <add>
        <rpn>anon:Anonymous, mf:Anonymous, +</rpn>
        <graphType>line</graphType>
        <color>blue</color>
        <legend>Anonymous memory usage</legend>
    </add>
    <add>
        <name>mf:Pss</name>
    </add>
    <add>
        <name>anon:Pss</name>
    </add>
    <add>
        <rpn>anon:Pss, mf:Pss, +</rpn>
        <graphType>line</graphType>
        <color>green</color>
        <legend>Resident size</legend>
    </add>
    <add>
        <name>mf:Swap</name>
    </add>
    <add>
        <name>anon:Swap</name>
    </add>
    <add>
        <rpn>anon:Swap, mf:Swap, +</rpn>
        <graphType>line</graphType>
        <color>red</color>
        <legend>Swapped memory</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>SYSTEM</pathelement>
        <pathstring>Processus</pathstring>
        <pathelement>INDEX</pathelement>
        <pathstring>Detailled memory usage</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>SYSTEM</pathelement>
        <pathstring>Processus</pathstring>
        <pathelement>HOST</pathelement>
        <pathelement>INDEX</pathelement>
        <pathstring>Detailled memory usage</pathstring>
    </viewtree>
</graphdesc>
